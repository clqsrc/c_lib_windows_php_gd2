CXXINCS   = -I"C:/Dev-Cpp/MinGW32/include" -I"C:/Dev-Cpp/" -I"C:/Dev-Cpp/include/common" -I"D:/test3/gd_test1" -I"D:/test3/libiconv_git/include"
