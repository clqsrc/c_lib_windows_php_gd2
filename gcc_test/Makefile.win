OBJ       = Objects/MingW/main.o Objects/MingW/utf8.o
LINKOBJ   = "Objects/MingW/main.o" "Objects/MingW/utf8.o"
LIBS      = -L"C:/Dev-Cpp/lib/wx/gcc_lib" -L"C:/Dev-Cpp/lib" -L"C:/Dev-Cpp/MinGW32/lib" -L"D:/test3/gd_test1/Release_TS" -L"D:/test3/libiconv_git/src111" -L"D:/test3/libiconv_git/lib" -l php_gd2 -l iconv  
